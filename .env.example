# Local Development Environment
ENVIRONMENT=local

# Local Supabase (from npx supabase status)
SUPABASE_API_URL=http://127.0.0.1:54321
SUPABASE_SECRET_KEY=your_supabase_secret_key

# Clerk (optional for local dev)
CLERK_SECRET_KEY=your_clerk_secret_key
DOMAIN="http://localhost:8000"

# Local S3 (from npx supabase status)
SUPABASE_STORAGE_URL="http://127.0.0.1:54321/storage/v1"
S3_BUCKET_NAME="rag-app-bucket"
AWS_REGION="local"
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_ENDPOINT_URL_S3="http://127.0.0.1:54321/storage/v1/s3"

# Optional services
REDIS_URL=
OPENAI_API_KEY=
SCRAPINGBEE_API_KEY=
TAVILY_API_KEY=

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Ollama Models (Local)
OLLAMA_LLM_MODEL=gemma3:4b
OLLAMA_EMBEDDING_MODEL=embeddinggemma:300m
